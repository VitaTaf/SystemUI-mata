<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="26.0dip" />
            <solid android:color="@color/sprint_smf_primary_button_pressed" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="26.0dip" />
            <stroke android:width="1.0dip" android:color="@android:color/black" />
            <solid android:color="@color/sprint_smf_primary_button" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="26.0dip" />
            <solid android:color="@color/sprint_smf_primary_button" />
        </shape>
    </item>
</selector>